{
  "id": "project-overview",
  "name": "View Project Overview",
  "module": "projects",
  "prerequisites": ["auth-login", "project-create"],
  "steps": [
    {
      "action": "navigate",
      "value": "http://localhost:3001/ryox-carpentry/projects/b694bd30-93d0-41dc-bda6-aadc9fa3ca57",
      "description": "Navigate to Project Overview page"
    },
    {
      "action": "wait",
      "value": "2000",
      "description": "Wait for overview page to load"
    },
    {
      "action": "assert",
      "selector": "heading:has-text('test')",
      "description": "Verify project name displays"
    },
    {
      "action": "assert",
      "selector": "text=Budget",
      "description": "Verify budget section displays"
    },
    {
      "action": "assert",
      "selector": "text=Timeline",
      "description": "Verify timeline section displays"
    },
    {
      "action": "assert",
      "selector": "text=Location",
      "description": "Verify location section displays"
    },
    {
      "action": "assert",
      "selector": "text=Project Workflows",
      "description": "Verify workflows section displays"
    },
    {
      "action": "click",
      "selector": "link:has-text('RFIs')",
      "description": "Navigate to RFIs from overview",
      "screenshot": true
    },
    {
      "action": "wait",
      "value": "2000",
      "description": "Wait for RFIs page to load"
    },
    {
      "action": "assert",
      "selector": "heading:has-text('RFIs')",
      "description": "Verify navigation to RFIs worked"
    }
  ],
  "cleanup": []
}
